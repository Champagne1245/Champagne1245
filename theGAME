</video>

<script>
var vid = document.getElementById("myVideo");

function enablePreload() { 
    vid.preload = "auto";
} 

function disablePreload() { 
    vid.preload = "none";
} 

function checkPreload() { 

    alert(vid.preload);
} 
</script> 

