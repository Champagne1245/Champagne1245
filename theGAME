<img id="myImage" onclick="changeImage()" src="red botton 2.jpeg" width="200" height="180">
<script>
function changeImage() {
    var image = document.getElementById('myImage');
    if (image.src.match("red botton 2")) {
        image.src = "red botton 2 explode.gif";
    } else {
        image.src = "red botton 2.jpeg";
    }
}
    </script>
